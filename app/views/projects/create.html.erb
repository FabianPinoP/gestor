<% if @project.errors.any? %>
    <div id="error_explanation">
        <h2>
            <%= pluralize(@project.errors.count, "error") %>
            prohibited this user from being saved:
        </h2>
        <ul>
            <% @project.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
            <% end %>
        </ul>
    </div>
    <% else %> 
    <h1>Proyecto ingresado con exito!!</h1>
<% end %>

